{
  "name": "retry-library",
  "version": "1.1.3",
  "description": "A library that uses an exponential backoff logic to cancel an API request if the response is delayed and retries the request a certain number of times.",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
    "compile": "rm -rf dist && mkdir dist && tsc"
  },
  "keywords": [
    "typescript",
    "retry"
  ],
  "author": "yeonssu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeonsuu21/Retry-module.git"
  },
  "bugs": {
    "url": "https://github.com/yeonsuu21/Retry-module/issues"
  },
  "homepage": "https://github.com/yeonsuu21/Retry-module#readme",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
